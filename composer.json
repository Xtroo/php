{
  "name": "xtroo/php-client",
  "description": "Xtroo PHP Client Library",
  "keywords": [
    "xtroo",
    "content extraction",
    "article extraction",
    "text processing"
  ],
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Tim Igoe",
      "email": "tim@xtroo.io",
      "homepage": "http://xtroo.io",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "php-http/client-implementation": "^1.0",
    "php-http/client-common": "^1",
    "php-http/discovery": "^1",
    "php-http/message": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "phpunit/php-code-coverage": "^5"
  },
  "autoload": {
    "psr-4": {
      "Xtroo\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xtroo\\Test\\": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
